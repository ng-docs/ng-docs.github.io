                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                               \n      </td>\n    </tr>\n  </tbody>\n</table>\n\n  \n  <h6 class=\"no-anchor\" id=\"返回值\">返回值</h6>\n  <p><code>Promise&#x3C;PushSubscription></code></p>\n\n  \n\n\n  \n\n  \n</div>\n      </td>\n    </tr>\n  \n\n  \n\n  \n    </tbody>\n</table>\n\n  \n    <a id=\"unsubscribe\"></a>\n<table class=\"is-full-width method-table instance-method\">\n  <thead><tr><th>\n    <div class=\"with-github-links\">\n    <h3 id=\"unsubscribe\">\n      unsubscribe()\n      \n    <a title=\"Link to this heading\" class=\"header-link\" aria-hidden=\"true\" href=\"api/service-worker/SwPush#unsubscribe\"><i class=\"material-icons\">link</i></a></h3>\n    <div class=\"github-links\">\n  <a href=\"https://github.com/angular/angular/edit/aio/packages/service-worker/src/push.ts?message=docs(service-worker)%3A%20describe%20your%20change...#L100-L121\" aria-label=\"提供编辑建议\" title=\"提供编辑建议\"><i class=\"material-icons\" aria-hidden=\"true\" role=\"img\">mode_edit</i></a>\n  <a href=\"https://github.com/angular/angular/tree/7.2.0-rc.0/packages/service-worker/src/push.ts#L100-L121\" aria-label=\"查看源码\" title=\"查看源码\"><i class=\"material-icons\" aria-hidden=\"true\" role=\"img\">code</i></a>\n</div>\n    </div>\n  </th></tr></thead>\n  <tbody>\n    \n  \n    <tr>\n      <td>\n        <div class=\"overload-info\">\n  \n\n  <code-example language=\"ts\" hidecopy=\"true\" linenums=\"false\" class=\"no-box api-heading\"><span class=\"member-name\">unsubscribe</span>(): Promise&#x3C;void></code-example>\n\n  \n\n  <h6 class=\"no-anchor\" id=\"参数-1\">参数</h6>\n  <p>没有参数。</p>\n\n  \n  <h6 class=\"no-anchor\" id=\"返回值-1\">返回值</h6>\n  <p><code>Promise&#x3C;void></code></p>\n\n  \n\n\n  \n\n  \n</div>\n      </td>\n    </tr>\n  \n\n  \n\n  \n    </tbody>\n</table>\n\n  \n</section>\n\n\n\n  \n\n\n  </div>\n</article>\n\n<!-- links to this doc:\n - api/common/I18nPluralPipe\n - api/router/DefaultUrlSerializer\n - api/service-worker\n - api/service-worker/ServiceWorkerModule\n - guide/component-interaction\n - guide/observables\n - guide/observables-in-angular\n - guide/pipes\n - guide/router\n - guide/rx-library\n - tutorial/toh-pt4\n - tutorial/toh-pt5\n - tutorial/toh-pt6\n-->\n<!-- links from this doc:\n - /api\n - api/service-worker\n - api/service-worker/ServiceWorkerModule\n - api/service-worker/SwPush#isEnabled\n - api/service-worker/SwPush#messages\n - api/service-worker/SwPush#notificationClicks\n - api/service-worker/SwPush#requestSubscription\n - api/service-worker/SwPush#requestsubscription\n - api/service-worker/SwPush#subscription\n - api/service-worker/SwPush#swpush\n - api/service-worker/SwPush#unsubscribe\n - api/service-worker/SwPush#属性\n - api/service-worker/SwPush#方法\n - https://developer.mozilla.org/en-US/docs/Web/API/Notification\n - https://developer.mozilla.org/en-US/docs/Web/API/PushSubscription\n - https://github.com/angular/angular/edit/aio/packages/service-worker/src/push.ts?message=docs(service-worker)%3A%20describe%20your%20change...#L100-L121\n - https://github.com/angular/angular/edit/aio/packages/service-worker/src/push.ts?message=docs(service-worker)%3A%20describe%20your%20change...#L14-L124\n - https://github.com/angular/angular/edit/aio/packages/service-worker/src/push.ts?message=docs(service-worker)%3A%20describe%20your%20change...#L80-L99\n - https://github.com/angular/angular/tree/7.2.0-rc.0/packages/service-worker/src/push.ts#L100-L121\n - https://github.com/angular/angular/tree/7.2.0-rc.0/packages/service-worker/src/push.ts#L14-L124\n - https://github.com/angular/angular/tree